#include <stdio.h>

// Function to find nth Fibonacci number
int fibonacci(int n) {
    if (n <= 1) return n;
    int a = 0, b = 1, c;
    for (int i = 2; i <= n; i++) {
        c = a + b;
        a = b;
        b = c;
    }
    return b;
}

int main() {
    int n;
    printf("Enter number of terms: ");
    scanf("%d", &n);

    int sum = fibonacci(n + 2) - 1;
    printf("Sum of Fibonacci series up to %d terms = %d\n", n, sum);

    return 0;
}
